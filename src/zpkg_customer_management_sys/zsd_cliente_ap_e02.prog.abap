MODULE pbo_9001 OUTPUT.
  CLEAR gd_okcode.
 SET PF-STATUS 'S9001'.
 SET TITLEBAR 'T9001'.
ENDMODULE.
*&---------------------------------------------------------------------*
*&      Module  PAI_9001  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE pai_9001 INPUT.

  CASE gd_okcode.
    WHEN 'BACK'.
       LEAVE TO SCREEN 0.
    WHEN 'SAVE'.
    WHEN 'EXEC' OR ''.
      PERFORM modificar_cliente.
  ENDCASE.

ENDMODULE.
